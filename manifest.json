{
  "name":"Youtube : Ad-skipper",
  "short_name": "Ad-skipper",
  "description":"Skips all the skippable ads on youtube without you have to wait for five seconds",
  "manifest_version": 2,
  "version":"1.0.0",
  "author":"Nikhil Singh",
  "content_scripts":[
    {
      "matches": ["*://*.youtube.com/*"],
      "js":["skip.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "icons" : {
    "128" : "icon128.png",
    "48" : "icon48.png",
    "16" : "icon16.png"
  }
}



